<tr class="<%= cycle('odd', 'even') %>">
  <td><%=h user.login %></td>
  <td><%=h user.email %></td>
  <td>(<%= link_to 'edit roles', user_roles_path(user) %>)</td>
  <td><% unless user.id == 1%> (<%= link_to 'delete user', user, :confirm => 'Are you sure?', :method => :delete %>)<% end %> </td>
</tr>
